{
  "name": "semantic-web-chatbot",
  "version": "0.1.0",
  "private": true,
  "author": "DarKPhuRioN",
  "description": "semantic web",
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "electron-is-dev": "^2.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.3",
    "react-simple-chatbot": "^0.6.1",
    "sass": "^1.32.8",
    "styled-components": "^5.2.1",
    "web-vitals": "^1.0.1"
  },
  "homepage": "./",
  "main": "public/electron.js",
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build": "react-scripts build",
    "start-electron-os": "BROWSER=none && react-scripts start",
    "start-electron-win": "set BROWSER=none && react-scripts start",
    "electron-dev": "concurrently \"npm run start-electron-os\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-dev-win": "concurrently \"npm run start-electron-win\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-build": "electron-builder"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^6.0.0",
    "electron": "^12.0.1",
    "wait-on": "^5.2.1"
  },
  "build": {
    "appId": "chatbotid",
    "win": {
      "icon": "ico/icon.ico"
    },
    "directories": {
      "buildResources": "public"
    }
  }
}
